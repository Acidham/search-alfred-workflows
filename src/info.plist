				<key>script</key>
				<string># Split string into lines
split_string() {
    echo "$1" | tr '\n' '\0' | xargs -0 -n1 echo
}

# Filter lines on &lt;string&gt; tags, pick the first one, return the enclosed string
get_bundleid() {
    local lines=("${(@f)1}")
    local filtered_lines=()
    for line in $lines; do
        if [[ $line == *"&lt;string&gt;"* ]] &amp;&amp; [[ $line == *"&lt;/string&gt;"* ]]; then
            filtered_lines+=("$line")
        fi
    done
	local uid_line="${filtered_lines[1]}"
	echo "${${uid_line#*&lt;string&gt;}%&lt;/string&gt;*}"
}

readonly dirpath="$plist_path"
readonly actual_plist_path="${dirpath}/info.plist"
readonly plist=$(&lt;"$actual_plist_path")
readonly workflow_bundleid=$(get_bundleid "$plist")

readonly cache_path="$HOME/Library/Caches/com.runningwithcrayons.Alfred/Workflow Data/$workflow_bundleid"
readonly data_path="$HOME/Library/Application Support/Alfred/Workflow Data/$workflow_bundleid"

cache_exists="false"
data_exists="false"

[[ -d $cache_path ]] &amp;&amp; cache_exists="true"
[[ -d $data_path ]] &amp;&amp; data_exists="true"

echo -n "${workflow_bundleid},${cache_exists},${data_exists}"</string>
